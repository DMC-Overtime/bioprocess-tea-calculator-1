    <div class ="item">
    <button class="submit-btn">Export Data</button>
    <div class="process content">
    <h2><%=pid.id %></h2>
    <div class="process details">
    <p><%=pid.strain %></p>
    </div>
    <a class="delete" data-doc="<%= pid._id %>">delete</a>
    </div>

    </div>

<br>
