<br>
<%- include('./partials/nav2.ejs') %>
 <script>
    const trashcan = documents.querySelector('a.delete');
    trashcan.addEventListener('click',(e) => {
        const endpoint = `/processes/$(trashcan.dataset.doc)`;
        fetch(endpoint,{
        method: 'DELETE'
        }).then((response) => response.json()).then((data)) => window.location.href=data.redirect).catch(err => console.log(err));
    })
 </script>

</body>
</html>
